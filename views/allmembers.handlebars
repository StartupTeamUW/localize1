{{> nav/navbar}}
{{!-- kevin's new card --}}
<br><br>
<ul id="user-list">
  {{#each users}}
  <li data-id="{{this.id}}" class="list-group-item">
    <a href="user/{{this.id}}">
      <div class="container">
        <div class="trip-summary-container s12 l10">
          <div class="profile-pic-container">
            <div class="profile-pic">
              <img src="{{user.profile_pic_url}}" style="float: left;" />
            </div>
          </div>
          <div class="profile-data">
            <h2 class="display-4 profile-name">{{this.user_name}}</h2>
            <h5 class="user-card-city">{{this.hometown}}</h5> <span class="guide-badge blue">Guide</span> <a href="#"><img
                class="contact" src="/imgs/kisspng-ipma-email-telephone-message-email-icon-5acf36458d39e5.4305648115235292855785.png"
                alt=""></a>

            <p class="lead">{{this.bio}}</p>
            <div class="interest-tokens">
              <ul>

                <li class="interest-badge red" id="fish"> <span class="">Fish</span></li>
                <li class="interest-badge red" id="Bars"> <span class="">Bars</span></li>

              </ul>
            </div>
          </div>
        </div>
      </div>
      <br>
      <br>
  </li>
  {{/each}}
</ul>
<a href="/">← Back To Home</a>



