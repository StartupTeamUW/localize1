<div class="row">
  <div class="col s12">This div is 12-columns wide on all screen sizes</div>
  <div class="col s6">6-columns (one-half)
    {{> nav/navbar}}
    <br><br>
    <ul id="user-list">
      {{#each user}}
      <li data-id="{{this.id}}" class="list-group-item">
        <a href="user/{{this.id}}">
          <div class="container">
            <div class="trip-summary-container s12 l10">
              <div class="profile-pic-container">
                <div class="profile-pic">
                  <img src="{{this.profile_pic_url}}" style="float: left;" />
                </div>
              </div>
              <div class="profile-data">
                <h2 class="display-4 profile-name">{{this.user_name}}</h2>
                <h5 class="user-card-city">{{this.hometown}}</h5> <span class="guide-badge blue">Guide</span> <a href="#"><img
                    class="contact" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQ0isRu8iqDzjlJJ2SYvDFcF2dReTmEKnP8mqSyAYv8vE98nYuK"
                    alt=""></a>
                <p class="lead">{{this.bio}}</p>
              </div>
            </div>
          </div>
          <br>
          <br>
      </li>
      {{/each}}
    </ul>
    <a href="/">← Back To Home</a>


