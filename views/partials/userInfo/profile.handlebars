<div class="container">
  <div class="trip-summary-container s12 l10">
    <div class="profile-pic">
      <img src="{{this.profile_pic_url}}" style="float: left;" />
    </div>
    <div class="profile-data">
      <h2 class="display-4 profile-name">{{this.user_name}}</h2>
      <h5>{{this.hometown}}</h5>
      <p class="lead">{{this.bio}}</p>
      <div class="interest-tokens">
        <ul>
          {{#each this_interests}}
          <li class="interests" id="{{this.text}}"> <span class="badge badge-pill badge-primary">{{this.text}}</span></li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>
<br>